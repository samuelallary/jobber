<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">

<channel>
  <title>Jobs for {{.Keywords}} in {{.Location}}</title>
  <link>https://websitename.com</link>
  <description>{{.Keywords}} in {{.Location}}</description>
  {{ range .Offers }}
  <item>
    <title>{{title .}}</title>
    <link>https://www.linkedin.com/jobs/view/{{.ID}}</link>
    <pubDate>{{createdAt .}}</pubDate>
    <guid isPermaLink="false">{{.ID}}</guid>
  </item>
  {{ end }}
</channel>
</rss>
